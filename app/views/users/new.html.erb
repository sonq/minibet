
<%= render 'shared/errors',obj: @user %>
<br/>
<div class = 'container-fluid'>

  <div class = 'col-xs-12'>

    <%= form_for(@user, :html => {class: "form-horizontal", role: "form"}) do |n| %>
      <div class = "form-group">
                <h1 class= "center"> Sign Up </h1>
        <div class = "control-label col-sm-4">
          <%= n.label :'User Name' %> 
        </div>
        <div class="col-sm-4">
          <%= n.text_field :username, :autocomplete => :off, class: "form-control" , placeholder: "User Name"  %>
        </div>
     
      </div> 

    


   


   




      <div class = "form-group">
        <div class = "control-label col-sm-4">
          <%= n.label :password %> 
        </div>
        <div class="col-sm-4">
          <%= n.password_field :password, :autocomplete => :off, class: "form-control", placeholder: "Password"  %>
        </div>
      </div>

      <div class = "form-group">
        <div class = "col-sm-offset-4 col-sm-12">
          <%= n.submit "Sign Up" , class: 'btn btn-primary' %>
        </div>
      </div>

      

      </div>
   


    </div>



    <% end %>
    





  </div>

</div>
</div>